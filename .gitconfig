[user]
	email = alexandern.aldana@gmail.com
	name = Alex Aldana
[alias]
	# Commit
 	c = commit
 	ca = commit -a
 	cm = commit -m
	cam = commit -am

	# Branch
	br = branch
	del = branch -D

	# Checkout
	co = checkout
	cob = checkout -b

	# Diff
	d = diff
	dc = diff --cached

	# Status
	s = status
	ss = status -sbu

	# Log
	l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit

	# Push
	done = push -u origin HEAD
	remote-del = push -d

[pull]
	rebase = true
